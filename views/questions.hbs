{{#if user}}
  <p> You are logged in as {{user.username}}! </p>
{{/if}}
<h2> Here is the Questions for students page! </h2>
<form role="form" action="/register" method="POST" style="max-width: 300px;">
  <div class='form-group'>
    <label>First Question Here<label>
        <input class='form-control' type="text" name="question1" placeholder="answer1">
  </div>
  <div class='form-group'>
    <label>Second Question Here<label>
        <input class='form-control' type="text" name="question2" placeholder="answer2">
  </div>
  <div class='form-group'>
    <label>Third Question Here<label>
        <input class='form-control' type="text" name="question3" placeholder="answer3">
  </div>
  <button class="btn btn-default" type="submit">Submit</button> &nbsp;
  <a href="/">
    <button class="btn btn-primary" type="button">Cancel</button></a>
</form>
